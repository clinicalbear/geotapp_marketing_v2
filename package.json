{
  "name": "geotapp-marketing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "node -e \"try{require('fs').rmSync('app/sitemap.xml',{recursive:true,force:true})}catch{} try{require('fs').rmSync('app/robots.txt',{recursive:true,force:true})}catch{} try{require('fs').rmSync('src/app/sitemap.xml',{recursive:true,force:true})}catch{} try{require('fs').rmSync('src/app/robots.txt',{recursive:true,force:true})}catch{}\" && next build --no-lint",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "analyze": "ANALYZE=true next build",
    "pages:build": "npx @cloudflare/next-on-pages@latest",
    "deploy": "npx wrangler@latest pages deploy .vercel/output --project-name geotapp-marketing"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "@cloudflare/next-on-pages": "latest",
    "wrangler": "latest"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/material-symbols-outlined": "^5.2.28",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/material-nextjs": "^7.3.3",
    "next": "^15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
